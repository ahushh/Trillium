{
    "filters": {
        "global": {
            "yui-path": "/usr/share/yui-compressor/yui-compressor.jar",
            "java-path": "/usr/bin/java"
        }
    },
    "css": {
        "filters": {
            "*": ["yui-css-compressor"]
        },
        "priority": {
            "vendor/jquery-terminal/jquery.terminal.css": 0,
            "application/style.css": 1
        }
    },
    "js": {
        "filters": {
            "*": ["yui-js-compressor"]
        },
        "priority": {
            "vendor/jquery/jquery-1.11.0.js": 0,
            "vendor/jquery-mousewheel/jquery.mousewheel.js": 1,
            "vendor/jquery-terminal/jquery.terminal-0.8.7.js": 2,
            "application/trillium.js": 3,
            "application/url-generator.js": 4
        }
    }
}