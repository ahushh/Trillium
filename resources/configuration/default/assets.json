{
    "filters": {
        "global": {
            "yui-path": "/usr/share/yui-compressor/yui-compressor.jar"
        }
    },
    "java-path": "/usr/bin/java",
    "vendor/bootstrap/bootstrap.css": {
        "filters": ["yui-css-compressor"]
    },
    "vendor/bootstrap/bootstrap-theme.css": {
        "filters": ["yui-css-compressor"]
    },
    "application/trillium.js": {
        "priority": 0,
        "filters": ["yui-js-compressor"]
    },
    "application/url-generator.js": {
        "priority": 1,
        "filters": ["yui-js-compressor"]
    }
}