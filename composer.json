{
    "name": "trillium/trillium",
    "type": "project",
    "keywords": ["imageboard", "engine", "imageboard engine"],
    "description": "Imageboard engine based on the Symfony2 components",
    "license": "MIT",
    "authors": [
        {
            "name": "Kilte Leichnam",
            "email": "nwotnbm@gmail.com"
