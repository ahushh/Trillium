        "fabpot/php-cs-fixer": "~0.4",
        "kilte/view": "~1.0",
        "kriswallsmith/assetic": "~1.1",
        "monolog/monolog": "~1.7",
        "pimple/pimple": "~2.0",
        "psr/log": "~1.0",
        "symfony/class-loader": "~2.4",
        "symfony/config": "~2.4",
        "symfony/console": "~2.4",
        "symfony/debug": "~2.4",
        "symfony/event-dispatcher": "~2.4",
        "symfony/filesystem": "~2.4",
        "symfony/finder": "~2.4",
        "symfony/http-foundation": "~2.4",
        "symfony/http-kernel": "~2.4",
        "symfony/process": "~2.4",
        "symfony/routing": "~2.4",
        "symfony/security": "~2.4",
        "symfony/translation": "~2.4",
        "symfony/yaml": "~2.4"
