{
    "filters": {
        "global": {
            "yui-path": "/usr/share/yui-compressor/yui-compressor.jar",
            "java-path": "/usr/bin/java"
        }
    },
    "black.css": {
        "set": [
            ["application/css/black.css", "yui-css-compressor"]
        ]
    },
    "white.css": {
        "set": [
            ["application/css/white.css", "yui-css-compressor"]
        ]
    },
    "scripts.js": {
        "default_filters": "yui-js-compressor",
        "set": [
            "vendor/jquery/jquery-1.11.0.js",
            "vendor/jquery-mousewheel/jquery.mousewheel.js",
            "vendor/jquery-terminal/jquery.terminal-0.8.7.js",
            "vendor/jquery-cookie/jquery.cookie.js",
            "application/js/trillium.js",
            "application/js/settings.js",
            "application/js/url-generator.js",
            "application/js/generated/settings.js",
            "application/js/generated/url-generator.js",
            "application/js/terminal.js",
            "application/js/commands/"
        ]
    }
}